  pixel1.setPixelColor(19, pixel1.Color(red,green,blue)); 
  pixel1.setPixelColor(21, pixel1.Color(red,green,blue)); 
  pixel1.setPixelColor(22, pixel1.Color(red,green,blue)); 
  pixel1.setPixelColor(23, pixel1.Color(red,green,blue));  
