  pixel2.setPixelColor(12, pixel2.Color(red,green,blue)); 
  pixel2.setPixelColor(13, pixel2.Color(red,green,blue)); 
  pixel2.setPixelColor(14, pixel2.Color(red,green,blue)); 
  pixel2.setPixelColor(19, pixel2.Color(red,green,blue)); 
  pixel2.setPixelColor(21, pixel2.Color(red,green,blue)); 
  pixel2.setPixelColor(22, pixel2.Color(red,green,blue)); 
  pixel2.setPixelColor(23, pixel2.Color(red,green,blue));  
