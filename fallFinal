      pixel2.setPixelColor(11, pixel2.Color(red,green,blue));
      pixel2.setPixelColor(12, pixel2.Color(red,green,blue));
      pixel2.setPixelColor(13, pixel2.Color(red,green,blue));
      pixel2.setPixelColor(14, pixel2.Color(red,green,blue));
      pixel2.show();                  

      delay(delayTime);

    //********** STEP FOUR *********//
      resetEyes();
      pixel1.setPixelColor(8, pixel1.Color(red,green,blue));
      pixel1.setPixelColor(12, pixel1.Color(red,green,blue));
      pixel1.setPixelColor(13, pixel1.Color(red,green,blue));
      pixel1.setPixelColor(16, pixel1.Color(red,green,blue));
      pixel1.setPixelColor(17, pixel1.Color(red,green,blue));
      pixel1.show();

      pixel2.setPixelColor(8, pixel2.Color(red,green,blue));
      pixel2.setPixelColor(12, pixel2.Color(red,green,blue));
      pixel2.setPixelColor(13, pixel2.Color(red,green,blue));
      pixel2.setPixelColor(16, pixel2.Color(red,green,blue));
      pixel2.setPixelColor(17, pixel2.Color(red,green,blue));
      pixel2.show();   
      delay(delayTime);
                    
    //********** STEP FIVE *********//
      resetEyes();
      pixel1.setPixelColor(1, pixel1.Color(red,green,blue));
      pixel1.setPixelColor(6, pixel1.Color(red,green,blue));
      pixel1.setPixelColor(7, pixel1.Color(red,green,blue));
      pixel1.setPixelColor(12, pixel1.Color(red,green,blue));
      pixel1.setPixelColor(13, pixel1.Color(red,green,blue));
      pixel1.setPixelColor(16, pixel1.Color(red,green,blue));
      pixel1.setPixelColor(17, pixel1.Color(red,green,blue));
      pixel1.setPixelColor(22, pixel1.Color(red,green,blue));
      pixel1.setPixelColor(23, pixel1.Color(red,green,blue));
      pixel1.show();
      
